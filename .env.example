# Environment Variables for Grape Banking Web App

# EmailJS Configuration (Frontend Email Service)
REACT_APP_EMAILJS_SERVICE_ID=service_your_service_id
REACT_APP_EMAILJS_TEMPLATE_ID=template_your_template_id
REACT_APP_EMAILJS_WAITLIST_TEMPLATE_ID=template_your_waitlist_template_id
REACT_APP_EMAILJS_PUBLIC_KEY=your_public_key_here

# Professional Email Settings
REACT_APP_EMAIL_FROM=your_email@yourdomain.com
REACT_APP_EMAIL_FROM_NAME=Your Company Name
REACT_APP_COMPANY_URL=https://yourdomain.com
REACT_APP_NOTIFICATION_EMAIL=notifications@yourdomain.com
REACT_APP_NOTIFICATION_EMAIL_NAME=Your Company Team

# Application Settings
REACT_APP_BASE_URL=https://yourdomain.com
REACT_APP_COMPANY_NAME=Your Company Name

# Feature Flags
REACT_APP_ENABLE_INVESTOR_PORTAL=true
REACT_APP_ENABLE_ENHANCED_FORMS=true
REACT_APP_APPS_LIVE=false
